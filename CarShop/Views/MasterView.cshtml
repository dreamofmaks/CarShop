
@{
	ViewBag.Title = "MasterView";
}

<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>Maserati</title>
	<link href="/Content/CSS/styles.css" rel="stylesheet" type="text/css" />
	<script src="js/jquery-1.11.0.min.js"></script>
	<script src="/Content/js/script.js"></script>
</head>
<body>
	<div id="wrapper">
		<div id="header">
			<img alt="Изображение" src="/Content/images/Header.png" />
		</div>
		<div id="content">
			<div id="nav">
				<ul>
					<li><a href="Home/Index"><img alt="Изображение" src="/Content/images/Logo.jpg" /></a></li>
					@Html.Action("Nav")
				</ul>
				<div id="map"><img src="/Content/images/Map.jpg" alt="Map"></div>
			</div>
			<div id="main">
				@RenderBody()
			</div>
		</div>
		<div id="footer">Copyrite</div>
	</div>
</body>
</html>
